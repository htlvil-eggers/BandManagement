import javafx.fxml.FXML;
import javafx.scene.web.WebView;

public class CalendarController {

    @FXML
    private WebView wviewCalendar;

    @FXML
    private void initialize() {
    	System.out.println(getClass().getResource("Site.html").toExternalForm());
    	this.wviewCalendar.getEngine().load(getClass().getResource("Site.html").toExternalForm());
    }
}
